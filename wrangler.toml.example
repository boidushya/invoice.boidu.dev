name = "invoice.boidu.dev"
main = "src/index.ts"
compatibility_date = "2024-01-01"

[[kv_namespaces]]
binding = "INVOICE_KV"
id = "your-kv-namespace-id"
preview_id = "your-preview-kv-namespace-id"

[vars]
ENVIRONMENT = "production"

# Include font assets - this will serve files from src/assets at the root
[assets]
directory = "./public/"
binding = "ASSETS"
